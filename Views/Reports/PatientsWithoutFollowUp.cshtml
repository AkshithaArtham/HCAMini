@model IEnumerable<dynamic>

<div class="container mt-4">
    <h2 class="text-primary mb-3">Patients Without Future Appointments</h2>

    <table class="table table-bordered table-hover">
        <thead class="table-dark">
            <tr>
                <th>Patient ID</th>
                <th>Name</th>
                <th>Gender</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var p in Model)
            {
                <tr>
                    <td>@p.PatientId</td>
                    <td>@p.FullName</td>
                    <td>@p.Gender</td>
                </tr>
            }
        </tbody>
    </table>
</div>
